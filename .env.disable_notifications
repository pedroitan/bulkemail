# Configuration to disable all notification systems
# Apply these settings by copying to .env or setting them directly

# Disable SQS processing entirely
DISABLE_SQS_PROCESSING=true

# Disable SNS notifications
DISABLE_SNS_NOTIFICATIONS=true

# Disable SES configuration set (prevents SNS feedback)
SES_CONFIGURATION_SET=

# Set this to true to disable return path tracking in SES
DISABLE_RETURN_PATH=true

# Disable email tracking entirely
DISABLE_EMAIL_TRACKING=true
